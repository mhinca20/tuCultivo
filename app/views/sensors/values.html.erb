<p id="notice"><%= notice %></p>

<h1>Values</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Value</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @values.each do |value| %>
      <tr>
        <td><%= value.id %></td>
        <td><%= value.value %></td>
        <td><%= value.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sensor', new_sensor_path %>