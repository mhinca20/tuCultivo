<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @sensor.name %>
</p>

<p>
  <strong>Node:</strong>
  <%= @sensor.node.name %>
</p>


<h1>values</h1>

<table>
  <thead>
    <tr>
      <th>Value</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @values.each do |value| %>
      <tr>
        <td><%= value.value %></td>
        <td><%= value.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<%= link_to 'Edit', edit_node_sensor_path(@sensor) %> |
<%= link_to 'Back', node_sensors_path %>
